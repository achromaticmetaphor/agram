<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="us.achromaticmetaphor.agram"
  android:installLocation="auto"
  android:screenOrientation="portrait"
  android:versionCode="21"
  android:versionName="1.4.1" >

  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

  <uses-sdk
    android:minSdkVersion="9"
    android:targetSdkVersion="25" />

  <application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher"
    android:theme="@style/Theme.AppCompat"
    android:label="@string/app_name" >
    <activity
      android:name="us.achromaticmetaphor.agram.MainActivity_"
      android:label="@string/app_name" >
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name="us.achromaticmetaphor.agram.About_"
      android:label="@string/app_name" >
    </activity>
    <activity
      android:name="us.achromaticmetaphor.agram.AboutText_"
      android:label="@string/app_name" >
    </activity>
    <activity
      android:name="us.achromaticmetaphor.agram.AnagramActivity_"
      android:label="@string/app_name" >
    </activity>
    <activity
      android:name="us.achromaticmetaphor.agram.FileBrowser_"
      android:label="@string/app_name" >
    </activity>
  </application>

</manifest>
